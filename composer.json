{
    "name": "df/doctrine-mongodb-provider",
    "description": "Dunkelfrosch fork of saxulum doctrine mongoDb provider for usage in PHP 7.n",
    "keywords": ["dunkelfrosch", "saxulum", "pimple", "silex", "doctrine", "mongo", "mongodb", "odm"],
    "license": "MIT",
    "type": "library",
    "time": "2016-04-25",
    "authors": [
        {
            "name": "Patrick Paechnatz",
            "email": "patrick.paechnatz@gmail.com"
        },
        {
            "name": "Dominik Zogg",
            "email": "dominik.zogg@gmail.com"
        }
    ],
    "replace": {
        "ext-mongo": "1.6.12"
    },
    "require": {
        "php": "^7.0",
        "pimple/pimple": "~3.0",
        "psr/log": "1.0.*",
        "alcaeus/mongo-php-adapter": "^1.0.0",
        "doctrine/mongodb": "dev-master",
        "mongodb/mongodb": "@beta"
    },
    "require-dev": {
        "phpunit/phpunit": "^4.7",
        "silex/silex": "2.0.x-dev"
    },
    "extra": {
        "branch-alias": {
          "dev-master": "2.0.x-dev"
        }
    },
    "autoload": {
      "psr-4": { "Saxulum\\DoctrineMongoDb\\": "src/" }
    }
}
